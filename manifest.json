{
    "manifest_version": 2,
    "name": "Vault",
    "version": "0.1",
    "description": "Stores almost-sensitive data in a portable text",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "browser_action": {
        "default_title": "Example Web Extension",
        "default_popup": "popup.html"
    }
}